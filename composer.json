{
  "name": "miniorange_inc/miniorange-security-suite",
  "description": "Miniorange Security Suite for Magento 2",
  "type": "magento2-module",
  "license": "OSL-3.0",
  "authors": [
      {
          "name": "ankush kumar",
          "email": "ankush@xecurify.com"
      }
  ],
  "homepage": "https://github.com/miniorange-inc/miniorange-security-suite",
  "keywords": [
      "Magento 2",
      "Magento",
      "Adobe Commerce",
      "Security",
      "Login",
      "Registration",
      "Password",
      "Email",
      "Phone",
      "OTP",
      "2FA"
  ],
  "require": {
      "php": "~7.4|~8.0|~8.1|~8.2|~8.3",
      "magento/framework": ">=103.0.4",
      "meilisearch/meilisearch-php": ">=v1.13.0"
  },
  "support": {
      "email": "magentosupport@xecurify.com"
  },
  "replace": {
      "miniorange/miniorange-social-login": "self.version",
      "miniorange/miniorange-2fa": "self.version",
      "miniorange_inc/security-suite": "self.version"
  },
  "autoload": {
      "files": [
          "miniorange-social-login/registration.php",
          "miniorange-2fa/registration.php",
          "security-suite/registration.php"
      ],
      "psr-4": {
          "MiniOrange\\MagentoSocialLogin\\": "miniorange-social-login",
          "MiniOrange\\TwoFA\\": "miniorange-2fa",
          "MiniOrange\\SecuritySuite\\": "security-suite"
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}